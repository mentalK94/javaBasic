	public void run() {
		
		card = new Card();
		System.out.println("수를 결정하였습니다. 맞추어 보세요");
		System.out.println("0-99");
		
		while(true) {
			count++;
			System.out.print(count+">>");
			input();
			result();
		}
	}
	
	public boolean result() {
		
	}
}
